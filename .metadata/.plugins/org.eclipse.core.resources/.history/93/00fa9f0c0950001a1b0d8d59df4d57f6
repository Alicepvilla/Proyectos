package practicas;

import java.util.Scanner;

public class Ejercicio4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	/*Llena un arreglo con números ingresados por el usuario (el primer valor corresponde al largo 
	 * del arreglo) y muéstralo.  Luego ordenalo de menor a mayor y muestra el resultado.
	 * 
	 */
		
		Scanner entrada = new Scanner (System.in);
		System.out.println("Dime de qué largo te gustaría ");
		int largo=entrada.nextInt();
		
		int numero=entrada.nextInt();
		
		while (numero<largo) {
			System.out.println("Ingresa número");
			break;
			
		}
	}

}
