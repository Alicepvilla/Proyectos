package practicas;

public class Ejercicio3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int [] arreglo = new int [6];
		
		for (int i = 0; i < arreglo.length; i++) {
			arreglo [i]=i+1;
		}
		
		for (int i = 0; i < arreglo.length; i++) {
			System.out.print("[" + arreglo[i] + "]");
		}
		
	}

}
